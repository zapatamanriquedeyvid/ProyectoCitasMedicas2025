package com.CitasMedicas.entidad;

import java.util.List;

import jakarta.persistence.*;

@Entity
@Table (name = "APODERADO")

public class Apoderado {

    @Id
    @GeneratedValue (strategy =GenerationType.IDENTITY)
    private int id_apod;
    private String tipo_documento;
    private int nro_documento;
    private String nombre_apod;
    private String apellido_apod;
    private String parentesco;
    private String celular_apod;
    private String correo_apod;

    @OneToMany(mappedBy = "apoderado",cascade = CascadeType.ALL)

    private List<Paciente> listaPacientes;

    public int getId_apod() {
        return id_apod;
    }
    public void setId_apod(int id_apod) {
        this.id_apod = id_apod;
    }
    public String getTipo_documento() {
        return tipo_documento;
    }
    public void setTipo_documento(String tipo_documento) {
        this.tipo_documento = tipo_documento;
    }
    public int getNro_documento() {
        return nro_documento;
    }
    public void setNro_documento(int nro_documento) {
        this.nro_documento = nro_documento;
    }
    public String getNombre_apod() {
        return nombre_apod;
    }
    public void setNombre_apod(String nombre_apod) {
        this.nombre_apod = nombre_apod;
    }
    public String getApellido_apod() {
        return apellido_apod;
    }
    public void setApellido_apod(String apellido_apod) {
        this.apellido_apod = apellido_apod;
    }
    public String getParentesco() {
        return parentesco;
    }
    public void setParentesco(String parentesco) {
        this.parentesco = parentesco;
    }
    public String getCelular_apod() {
        return celular_apod;
    }
    public void setCelular_apod(String celular_apod) {
        this.celular_apod = celular_apod;
    }
    public String getCorreo_apod() {
        return correo_apod;
    }
    public void setCorreo_apod(String correo_apod) {
        this.correo_apod = correo_apod;
    }
    public List<Paciente> getListaPacientes() {
        return listaPacientes;
    }
    public void setListaPacientes(List<Paciente> listaPacientes) {
        this.listaPacientes = listaPacientes;
    }

}