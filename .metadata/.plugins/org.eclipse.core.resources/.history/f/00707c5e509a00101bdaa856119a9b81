
create database PIZZERIA;
use  PIZZERIA;
CREATE TABLE `EMPLEADO` (
`id_emple` int NOT NULL AUTO_INCREMENT,
`nom_emple` varchar(45) NOT NULL, 
`ape_emple` varchar(45) NOT NULL, 
`correo_emple` varchar(45) NOT NULL, 
`contra_emple` varchar(45) NOT NULL,
`telef_emple` char(9) NOT NULL,
PRIMARY KEY (`id_emple`),
UNIQUE KEY `id_emple_UNIQUE` (`id_emple`),
UNIQUE KEY `correo_emple_UNIQUE` (`correo_emple`)
);

CREATE TABLE `TIENDA` (
`id_tienda` int NOT NULL AUTO_INCREMENT,
`nom_tienda` varchar(45) NOT NULL,
`direcc_tienda` varchar(100) NOT NULL,
PRIMARY KEY (`id_tienda`),  
UNIQUE KEY `id_tienda_UNIQUE` (`id_tienda`)
);

CREATE TABLE `CATEGORIA` (
`id_cate` int NOT NULL AUTO_INCREMENT,
`descrip_cate` varchar(45) NOT NULL,
PRIMARY KEY (`id_cate`),
UNIQUE KEY `id_cate_UNIQUE` (`id_cate`)
);

CREATE TABLE `CLIENTE` (
`id_cli` int NOT NULL AUTO_INCREMENT,
`nom_cli` varchar(45) NOT NULL,
`ape_cli` varchar(45) NOT NULL,
`correo_cli` varchar(45) NOT NULL,
`contra_cli` varchar(45) NOT NULL,
`direcc_cli` varchar(100) NOT NULL,
`telef_cli` char(9) NOT NULL,
PRIMARY KEY (`id_cli`),
UNIQUE KEY `id_cli_UNIQUE` (`id_cli`),
UNIQUE KEY `correo_cli_UNIQUE` (`correo_cli`)
);

CREATE TABLE `PRODUCTO` (
`id_prod` int NOT NULL AUTO_INCREMENT,
`nombre_prod` varchar(100) NOT NULL,
`descrip_prod` varchar(300) NOT NULL,
`precio_unit` decimal(6,2) NOT NULL,
`url_imagen` varchar(300) NOT NULL,
`id_cate` int NOT NULL,
PRIMARY KEY (`id_prod`),
UNIQUE KEY `id_prod_UNIQUE` (`id_prod`),
CONSTRAINT `id_cate` FOREIGN KEY (`id_cate`) REFERENCES `CATEGORIA` (`id_cate`)
);

CREATE TABLE `VENTA` (
`id_boleta` int NOT NULL AUTO_INCREMENT,
`num_boleta` char(23) NOT NULL,
`fecha_emi` datetime NOT NULL,
`tipo_pago` varchar(45) NOT NULL,
`total` decimal(6,2) NOT NULL,
`id_cli` int NOT NULL,
`id_tienda` int NOT NULL,
PRIMARY KEY (`id_boleta`),
UNIQUE KEY `id_boleta_UNIQUE` (`id_boleta`),
CONSTRAINT `id_cli` FOREIGN KEY (`id_cli`) REFERENCES `CLIENTE` (`id_cli`),
CONSTRAINT `id_tienda` FOREIGN KEY (`id_tienda`) REFERENCES `TIENDA` (`id_tienda`)
);

CREATE TABLE `DETALLE_VENTA` (
`id_prod` int NOT NULL,
`id_boleta` int NOT NULL,
`cantidad` int NOT NULL,
`subTotal` decimal(6,2) NOT NULL,
PRIMARY KEY (`id_prod`,`id_boleta`), 
CONSTRAINT `id_prod` FOREIGN KEY (`id_prod`) REFERENCES `PRODUCTO` (`id_prod`),
CONSTRAINT `id_boleta` FOREIGN KEY (`id_boleta`) REFERENCES `VENTA` (`id_boleta`)
);


SELECT*FROM EMPLEADO;
SELECT*FROM TIENDA;
SELECT*FROM CATEGORIA;
SELECT*FROM CLIENTE;
SELECT*FROM PRODUCTO;
SELECT*FROM VENTA;
SELECT*FROM DETALLE_VENTA;

delete from CLIENTE where id_cli= '16';
